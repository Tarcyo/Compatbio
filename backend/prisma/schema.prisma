generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement()) @db.UnsignedInt
  email String @unique @db.VarChar(255)
  name  String @map("nome") @db.VarChar(255)

  @@map("usuario")
}
